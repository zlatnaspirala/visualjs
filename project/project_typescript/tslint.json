{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": {
            "severity": "warning",
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace"
            ]
        },
        "indent": [true, "spaces", 2],
        "object-literal-sort-keys": false,
        "only-arrow-functions": false,
        "max-line-length": [true, {"limit": 140, "ignore-pattern": "^import |^export {(.*?)}"}],
        "one-variable-per-declaration": false
    },
    "rulesDirectory": []
}